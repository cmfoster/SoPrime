<div class="container">
	<%=simple_form_for @request_form, :class => 'form-horizontal' do |f|%>
		<%=f.input :fname%>
		<%=f.input :lname%>
		<%=f.input :email%>
		<%=f.input :phone%>
		<%=f.input :event_type%>
		<%=f.input :start_time%>
		<%=f.input :end_time%>
		<%=f.input :event_city%>
		<%=f.input :event_state%>
		<%=f.submit :submit%>
	<%end%>
</div>
